{
    "name": "@wetransfer/ghaction-gotestfmt",
    "version": "0.0.1",
    "main": "dist/index.js",
    "license": "MIT",
    "scripts": {
        "prepare": "husky install",
        "build": "webpack",
        "watch": "webpack --watch",
        "lint": "eslint ./src --fix",
        "lint:check": "eslint ./src",
        "check-types": "tsc --noEmit",
        "check-exports": "ts-unused-exports",
        "check-deps": "depcheck --ignore-bin-package"
    },
    "lint-staged": {
        "src/**/*.ts": [
            "eslint --fix",
            "prettier --write"
        ],
        "./*.js": [
            "prettier --write"
        ],
        "*.{json,yml}": [
            "prettier --write"
        ]
    },
    "devDependencies": {
        "@actions/core": "^1.10.0",
        "@actions/github": "^5.1.1",
        "@octokit/core": "^4.1.0",
        "@types/event-stream": "^4.0.0",
        "@types/node": "^18.11.5",
        "@types/signale": "^1.4.4",
        "@typescript-eslint/eslint-plugin": "^5.41.0",
        "@typescript-eslint/parser": "^5.41.0",
        "@vercel/ncc": "^0.34.0",
        "ansi-colors": "^4.1.3",
        "depcheck": "^1.4.3",
        "eslint": "^8.26.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-prettier": "^4.2.1",
        "eslint-plugin-simple-import-sort": "^8.0.0",
        "eslint-plugin-unused-imports": "^2.0.0",
        "event-stream": "^4.0.1",
        "husky": "^8.0.0",
        "lint-staged": "^13.0.3",
        "prettier": "^2.7.1",
        "signale": "^1.4.0",
        "ts-loader": "^9.4.1",
        "ts-node": "^10.9.1",
        "ts-toolbelt": "^9.6.0",
        "ts-unused-exports": "^8.0.0",
        "typescript": "^4.8.4",
        "webpack": "^5.74.0",
        "webpack-cli": "^4.10.0"
    },
    "packageManager": "yarn@3.2.4"
}
